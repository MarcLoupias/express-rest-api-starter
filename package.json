{
    "name": "express-rest-api-starter",
    "version": "0.0.0",
    "engines": {
        "node": ">=6.0.0",
        "npm": ">=5.0.0"
    },
    "private": true,
    "scripts": {
        "precommit": "npm test",
        "prepush": "npm test",
        "start": "node ./bin/www",
        "lint": "./node_modules/.bin/eslint -c .eslintrc.json **/*.js *.js",
        "pretest": "npm run-script lint",
        "test": "mocha test/**/*.js",
        "test-unit": "mocha test/unit/**/*.js",
        "test-int": "mocha test/integration/**/*.js"
    },
    "dependencies": {
        "body-parser": "1.17.2",
        "cookie-parser": "1.4.3",
        "debug": "2.6.8",
        "express": "4.15.3",
        "morgan": "1.8.2",
        "superagent": "3.6.0"
    },
    "devDependencies": {
        "eslint": "^4.6.1",
        "eslint-plugin-node": "^5.1.1",
        "husky": "^0.14.3",
        "mocha": "3.4.2"
    }
}
